{
  "version": "2.0.0",
  "configurations": [
    {
      "name": "Play in Editor",
      "type": "godot-mono",
      "mode": "playInEditor",
      "request": "launch",
      "justMyCode": "false"
    },
    {
      "name": "Launch",
      "type": "godot-mono",
      "request": "launch",
      "mode": "executable",
      "justMyCode": false,
      "requireExactSource": false,
      "suppressJITOptimizations": true,
      "symbolOptions": {
        "searchPaths": [],
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true
      },
      "preLaunchTask": "build",
      "executable": "<add your Godot path>",
      // See which arguments are available here:
      // https://docs.godotengine.org/en/stable/getting_started/editor/command_line_tutorial.html
      "executableArguments": [
        "--path",
        "${workspaceRoot}"
      ]
    },
    {
      "name": "Launch (Select Scene)",
      "type": "godot-mono",
      "request": "launch",
      "mode": "executable",
      "preLaunchTask": "build",
      "executable": "<add your Godot path>",
      "justMyCode": "false",
      // See which arguments are available here:
      // https://docs.godotengine.org/en/stable/getting_started/editor/command_line_tutorial.html
      "executableArguments": [
        "--path",
        "${workspaceRoot}",
        "${command:SelectLaunchScene}"
      ]
    },
    {
      "name": "Attach",
      "type": "godot-mono",
      "request": "attach",
      "justMyCode": "false",
      "address": "localhost",
      "port": 23685
    }
  ]
}